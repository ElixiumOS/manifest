<?xml version="1.0" encoding="UTF-8"?>
<!--*
    * This is the caf manifest for build ElixiumOS on caf devices.
    * If you want to compile ElixiumOS for a Nexus (or Pixel) device switch to the nexus-7.1 branch.
    * If you are building on MacOS you need to add the required toolchains from the AOSP manifest.
    * The manifest includes the Uber Toolchain.
    * Have a nice day
    * -->
<manifest>

    <default revision="refs/tags/android-7.1.1_r28" remote="aosp" sync-j="16" />

    <remote name="github"
        fetch="https://github.com" />

    <remote name="aosp"
        fetch="https://android.googlesource.com"
        review="android-review.googlesource.com" />

    <remote name="elixium-caf"
        fetch="https://github.com/ElixiumOS"
        revision="caf-7.1" />
    
    <remote name="discovery"
        fetch="https://github.com/DiscoveryTeam"
        revision="aosp-N-legacy" />

    <remote  name="du"
        fetch="https://github.com/DirtyUnicorns/"
        review="gerrit.dirtyunicorns.com" />

    <remote name="bitbucket"
            fetch="https://bitbucket.org"
            revision="master" />

    <remote name="aosp-caf"
        fetch="https://github.com/AOSP-CAF"
        revision="n-mr1" />

  <!-- The others manifests -->
  <include name="aosp.xml" />
  <include name="caf-lineage.xml" />

<!-- ElixiumOS -->

  <!-- Build -->
  <project path="build" name="platform_build" groups="pdk" remote="elixium-caf" >
        <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- Vendor -->
  <project path="vendor/elixium" name="ElixiumOS/vendor_elixium" remote="github" revision="elixium-7.1" >
        <linkfile src="builder.sh" dest="builder.sh" />
  </project>

  <!-- Packages -->
  <project path="packages/apps/Settings" name="ElixiumOS/platform_packages_apps_Settings" remote="github" revision="new-elixium-7.1" />
  <project path="packages/apps/DNA" name="ElixiumOS/packages_apps_DNA" remote="github" revision="elixium-7.1" />

  <!-- Frameworks -->
  <project path="frameworks/av" name="platform_frameworks_av" groups="pdk" remote="elixium-caf" />
  <project path="frameworks/base" name="platform_framework_base" groups="pdk-cw-fs,pdk-fs" remote="elixium-caf" />
  <project path="frameworks/native" name="platform_frameworks_native" groups="pdk" remote="elixium-caf" />
  <project path="frameworks/opt/telephony" name="android_frameworks_opt_telephony" remote="elixium-caf" />

  <!-- Devices -->
  <project path="device/oppo/common" name="device_oppo_common" remote="discovery" />
  <project path="device/oneplus/oneplus3" name="device_oneplus_oneplus3" remote="elixium-caf" />
  <project path="kernel/oneplus/msm8996" name="ElixiumOS/android_kernel_oneplus_msm8996" remote="github" revision="elix-discovery-7.1" />
  <project path="vendor/oneplus" name="proprietary_vendor_oneplus3" remote="discovery" />

  <!-- Systems -->
  <project path="system/core" name="android_system_core" groups="pdk" remote="elixium-caf" />
  
  <!-- Externals -->
  <project path="external/ipsec-tools" name="ElixiumOS/android_external_ipsec-tools" remote="github" revision="cm-14.1" />

  <!-- Misc -->
  <project path="bootable/recovery" name="nitrogen-project/android_bootable_recovery" remote="github" revision="n2" />

<!-- DirtyUnicorns -->

  <project path="bionic" name="android_bionic" remote="du" revision="n7x" />
  <project path="art" name="android_art" remote="du" revision="n7x" />

  <!-- Hardware -->
  <project path="hardware/libhardware" name="android_hardware_libhardware" remote="du" revision="n7x-caf" />
  <project path="hardware/broadcom/wlan" name="android_hardware_broadcom_wlan" remote="du" revision="n7x" />
  <project path="hardware/ril" name="android_hardware_ril" remote="du" revision="n7x-caf" />

  <!-- Packages -->
  <project path="packages/apps/Bluetooth" name="android_packages_apps_Bluetooth" remote="du" revision="n7x" />
  <project path="packages/apps/Contacts" name="android_packages_apps_Contacts" remote="du" revision="n7x" />
  <project path="packages/apps/ContactsCommon" name="android_packages_apps_ContactsCommon" remote="du" revision="n7x" />
  <project path="packages/apps/Dialer" name="android_packages_apps_Dialer" remote="du" revision="n7x" />
  <project path="packages/services/Telecomm" name="android_packages_services_Telecomm" remote="du" revision="n7x" />
  <project path="packages/services/Telephony" name="android_packages_services_Telephony" remote="du" revision="n7x" />
  <project path="packages/providers/TelephonyProvider" name="android_packages_providers_TelephonyProvider" groups="pdk-cw-fs,pdk-fs" remote="du" revision="n7x" />


  <!-- Frameworks -->
  <project path="frameworks/support" name="android_frameworks_support" remote="du" revision="n7x" />
  <project path="frameworks/opt/net/ims" name="android_frameworks_opt_net_ims" remote="du" revision="n7x" />
  <project path="frameworks/opt/net/wifi" name="android_frameworks_opt_net_wifi" remote="du" revision="n7x" />

  <!-- Systems -->
  <project path="system/bt" name="android_system_bt" remote="du" revision="n7x" />
  <project path="system/extras" name="android_system_extras" remote="du" revision="n7x-caf" />
  <project path="system/media" name="android_system_media" remote="du" revision="n7x-caf" />
  <project path="system/netd" name="android_system_netd" remote="du" revision="n7x-caf" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="du" revision="n7x" />
  <project path="system/vold" name="android_system_vold" remote="du" revision="n7x-caf" />

  <!-- Externals -->
  <project path="external/fuse" name="android_external_fuse" remote="du" revision="n7x-caf" />

<!-- Discovery ROM -->

  <!-- Externals -->
  <project path="external/android-clat" name="platform_external_android-clat" groups="pdk-cw-fs,pdk-fs" remote="discovery" />
  <project path="external/boringssl" name="platform_external_boringssl" groups="pdk" remote="discovery" />
  <project path="external/bzip2" name="platform_external_bzip2" groups="pdk" remote="discovery" />
  <project path="external/f2fs-tools" name="platform_external_f2fs-tools" groups="pdk" remote="aosp-caf" />
  <project path="external/ffmpeg" name="platform_external_ffmpeg" remote="discovery" />
  <project path="external/iproute2" name="platform_external_iproute2" groups="pdk" remote="discovery" />
  <project path="external/libselinux" name="platform_external_libselinux" groups="pdk" remote="discovery" />
  <project path="external/lz4" name="platform_external_lz4" groups="pdk-cw-fs,pdk-fs" remote="discovery" />
  <project path="external/lzma" name="platform_external_lzma" groups="pdk" remote="discovery" />
  <project path="external/okhttp" name="platform_external_okhttp" groups="pdk-cw-fs,pdk-fs" remote="discovery" />
  <project path="external/skia" name="platform_external_skia" groups="pdk-cw-fs,qcom_msm8x26" remote="discovery" />
  <project path="external/tinyxml2" name="platform_external_tinyxml2" groups="pdk" remote="discovery" />

  <!-- Packages -->
  <project path="packages/apps/FMRadio" name="platform_packages_apps_FMRadio" groups="pdk-fs" remote="discovery" />
  <project path="packages/apps/Launcher3" name="platform_packages_apps_Launcher3" groups="pdk-fs" remote="discovery" />
  <project path="packages/apps/Nfc" name="platform_packages_apps_Nfc" groups="apps_nfc,pdk-fs" remote="discovery" />
  <project path="packages/apps/PhoneCommon" name="platform_packages_apps_PhoneCommon" groups="pdk-cw-fs,pdk-fs" remote="discovery" />
  <project path="packages/apps/PackageInstaller" name="platform_packages_apps_PackageInstaller" groups="pdk-fs" remote="discovery" />
  <project path="packages/providers/ContactsProvider" name="platform_packages_providers_ContactsProvider" groups="pdk-cw-fs,pdk-fs" remote="discovery" />
  <project path="packages/providers/DownloadProvider" name="platform_packages_providers_DownloadProvider" groups="pdk-cw-fs,pdk-fs" remote="discovery"  />
  <project path="packages/providers/MediaProvider" name="platform_packages_providers_MediaProvider" groups="pdk-cw-fs,pdk-fs" remote="aosp-caf" />

  <!--System -->
  <project path="system/keymaster" name="platform_system_keymaster" groups="pdk" remote="discovery" />


<!-- UBERTC -->
  <!-- arm64 tc -->
  <project path="prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" name="DespairFactor/aarch64-linux-android-4.9" groups="pdk,linux,arm" remote="bitbucket" />
  <!-- arm tc -->
  <project path="prebuilts/gcc/linux-x86/arm/arm-eabi-4.9" name="DespairFactor/arm-eabi-4.9" groups="pdk,linux,arm" remote="bitbucket" />

<!-- Projekt -->
  <project path="packages/apps/ThemeInterfacer" name="substratum/interfacer" remote="github" revision="n-rootless" />

</manifest>
